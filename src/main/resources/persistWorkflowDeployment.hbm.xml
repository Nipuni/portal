<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.dhara.portal.web.dataAccessLayer.entity.PersistWorkflowDeployment" table="deployedWorkflows">
        <id name="persistDeploymentId" column="persist-deployment-id">
            <generator class="native">
            </generator>
        </id>
        <property name="deployedDate" column="deployed-date" type="timestamp"/>
        <property name="workflowId" column="workflow-id" type="string"/>
    </class>
</hibernate-mapping>



