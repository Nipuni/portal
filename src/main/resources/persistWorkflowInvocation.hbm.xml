<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.dhara.portal.web.dataAccessLayer.entity.PersistWorkflowInvocation" table="executedWorkflows">
        <id name="persistInvocationId" column="persist-invocation-id">
            <generator class="native">
            </generator>
        </id>
        <property name="executedDate" column="executed-datee" type="timestamp"/>
        <property name="workflowId" column="workflow-id" type="string"/>
        <property name="invocationId" column="workflow-invocation-id" type="string"/>
    </class>
</hibernate-mapping>